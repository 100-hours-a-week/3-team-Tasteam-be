# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
#
# JWT
JWT_SECRET=change-me
JWT_ACCESS_TOKEN_EXPIRATION=1800000
JWT_REFRESH_TOKEN_EXPIRATION=1209600000

# OAuth2 (선택)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
KAKAO_CLIENT_ID=
KAKAO_CLIENT_SECRET=
